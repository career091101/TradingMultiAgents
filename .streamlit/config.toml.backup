[global]
# Streamlit UI設定
developmentMode = false

[server]
# サーバー設定
port = 8501
enableCORS = false
enableXsrfProtection = true
maxUploadSize = 200

[browser]
# ブラウザ設定
gatherUsageStats = false

[theme]
# UI テーマ設定
primaryColor = "#FF6B6B"
backgroundColor = "#FFFFFF"
secondaryBackgroundColor = "#F0F2F6"
textColor = "#262730"
font = "sans serif"

[logger]
# ログ設定
level = "info"
messageFormat = "%(asctime)s %(message)s"

[client]
# クライアント設定
caching = true
displayEnabled = true
showErrorDetails = false

[runner]
# 実行設定
magicEnabled = true
installTracer = false
fixMatplotlib = true
postScriptGC = true
fastReruns = true
enforceSerializableSessionState = false

[mapbox]
# Mapbox設定（使用しない場合は空に）
token = ""

[deprecation]
# 廃止予定機能の設定
showPyplotGlobalUse = false
showfileUploaderEncoding = false

[s3]
# S3設定（使用しない場合は空に）
bucket = ""
url = ""
accessKeyId = ""
secretAccessKey = ""
region = ""

[sharing]
# 共有設定
email = ""